    <!--MSTECH
    *Resultado do ProvaSP destinado aos estudantes ou usuários de tipo ALUNO.

    OBS: Reparar que são montados gráficos específicos para os usuários ALUNOS
    *
    -->
    <div id="resultado-aluno-page" class="page generico_mainDiv">
        <div class="container generico_subDiv" style="text-align:center;">
            <img src="/AppProvaSP/images/logo_provaSP.png" class="generic_logo" alt="Prova São Paulo" />

            <div class="generico_headerDiv">
                <p class="generico_headerTitle">RESULTADOS</p>
                <p id="label_resultadoAluno" class="generico_headerText">Consulta de Resultado</p>
            </div>

            <form>
                <select id="ddlResultadoAlunoAreaConhecimento" data-mini="true">
                    <option value="" selected="selected">(Área de Conhecimento...)</option>
                    <option value="1">Ciências da Natureza</option>
                    <option value="2">Língua Portuguesa</option>
                    <option value="4">Redação</option>
                    <option value="3">Matemática</option>
                </select>

                <select id="ddlResultadoAlunoEdicao" data-mini="true">
                    <option value="" selected="selected">(Edição...)</option>
                </select>
            </form>


            <div id="divResultadoApresentacaoAluno" style="display:none; margin-top:15px;">
                <span class="lblResultadoTituloEscalaSaeb_1"></span>

                <div class="divChartResultadoEscalaSaeb_1"></div>

                <img class="imgChartResultadoDetalhe" />

                <div class="divChartResultadoDetalhe" style=""></div>

                <span class="lblResultadoTituloEscalaSaeb_2"></span>
                <div class="divChartResultadoEscalaSaeb_2"></div>

                <button id="btnProvaAlunoPopup" data-role="button" style="margin-top: 20px;">
                    Prova do Aluno<span class='mdi mdi-open-in-new'></span>
                </button>
            </div>

            <button id="btnAlunoVoltar" data-role="button" style="display: none;">Voltar</button>
        </div>
    </div>

    <!--MSTECH
    *Nova Div para configurações. Esta página terá as opções hoje determinadas por script no servidor
    da ProvaSP. Basicamente alterando duas configurações básicas.
    -->
    <div id="configuracoes-page" class="page generico_mainDiv" style="display: none;">
        <div class="container generico_subDiv">
            <img src="/AppProvaSP/images/logo_provaSP.png" class="generic_logo" alt="Prova São Paulo" />

            <div class="generico_headerDiv">
                <p class="generico_headerTitle">CONFIGURAÇÕES</p>
                <p class="generico_headerText">ACESSO NÍVEL SME</p>
            </div>

            <div class="configuracoes_optionsMainDiv">
                <!--Tab Configuração dos Níveis de Proficiência-->
                <p id="configuracoes_tab0" class="resultados_headerTitle" onclick="divConfiguracoes(0)">
                    NÍVEIS DE PROFICIÊNCIA
                    <span id="configuracoes_icone0">
                        <span class="mdi mdi-arrow-down-drop-circle-outline"></span>
                    </span>
                </p>
                <div id="configuracoes_div0" class="configuracoes_tabDiv" style="display: none;">
                    <!--Ano ou Ciclo da Proficiência-->
                    <p class="configuracoes_optionsTitleLeft">SELECIONAR ANO OU CICLO</p>
                    <select id="confAnoCiclo" data-mini="true">
                        <option value="" selected="selected">(Ano ou Ciclo...)</option>
                        <option value="2">2º Ano</option>
                        <option value="3">3º Ano</option>
                        <option value="4">4º Ano</option>
                        <option value="5">5º Ano</option>
                        <option value="6">6º Ano</option>
                        <option value="7">7º Ano</option>
                        <option value="8">8º Ano</option>
                        <option value="9">9º Ano</option>
                        <option value="c1">Ciclo de Alfabetização</option>
                        <option value="c2">Ciclo Interdisciplinar</option>
                        <option value="c3">Ciclo Autoral</option>
                    </select>
                    <!--Nível da Proficiência-->
                    <p class="configuracoes_optionsTitleLeft">SELECIONAR NÍVEL DA PROFICIÊNCIA</p>
                    <select id="confNivelProficiência" data-mini="true">
                        <option value="" selected="selected">(Nível de proficiência...)</option>
                        <option value="0">Nível indefinido</option>
                        <option value="1">Primeiro nível</option>
                        <option value="2">Segundo nível</option>
                        <option value="3">Terceiro nível</option>
                        <option value="4">Quarto nível</option>
                    </select>

                    <!--Nome-->
                    <p class="configuracoes_optionsTitleLeft">NOME DA PROFICIÊNCIA</p>
                    <input id="confProficienciaNome" type="text" placeholder="Ex: Nível Básico" />
                    <!--Descrição-->
                    <p class="configuracoes_optionsTitleLeft">DESCRIÇÃO DA PROFICIÊNCIA</p>
                    <textarea id="confProficienciaDescricao" placeholder="Ex: Desempenho padrão."></textarea>

                    <!--Enviar Proficiência-->
                    <button id="btnEnviarProficiencia" style="margin-top: 20px;"
                            data-role="button">
                        Atualizar proficiência
                    </button>
                </div>

                <!--Tab Configurações gerais-->
                <p id="configuracoes_tab1" class="resultados_headerTitle" onclick="divConfiguracoes(1)">
                    OPÇÕES PROVASP
                    <span id="configuracoes_icone1">
                        <span class="mdi mdi-arrow-down-drop-circle-outline"></span>
                    </span>
                </p>
                <div id="configuracoes_div1" class="configuracoes_tabDiv" style="display: none;">
                    <div class="configuracoes_optionsDiv"
                         onclick="alterarConfiguracao('DisponibilizarPreenchimentoQuestionariosFichas')">
                        <p class="configuracoes_optionsTitle">
                            PREENCHIMENTO DE QUESTIONÁRIOS: <span id="opcaoPreenchimentoQuestionarios"></span>
                        </p>
                        <p class="configuracoes_optionsText">
                            Permite o acesso às telas de preenchimento de questionários, sejam eles socioeconômicos
                            ou mesmo questionário de aplicação da ProvaSP. Desative esta opção para direcionar os usuários
                            à tela de <b>Resultados.</b>
                        </p>
                    </div>

                    <div class="configuracoes_optionsDiv"
                         onclick="alterarConfiguracao('RelatorioAcompanhamentoVisivel')">
                        <p class="configuracoes_optionsTitle">
                            RELATÓRIO ACOMPANHAMENTO: <span id="opcaoRelatorioAcompanhamento"></span>
                        </p>
                        <p class="configuracoes_optionsText">
                            Ative esta opção para permitir que o <b>Relatório de Acompanhamento</b> seja acessado pelos
                            usuários correspondentes. Geralmente esta opção deve ser habilitada durante a ProvaSP.
                        </p>
                    </div>

                    <div class="configuracoes_optionsDiv">
                        <p class="configuracoes_optionsTitle">
                            REPRESENTATIVIDADE SEGUNDO O INEP: <span id="opcaoRepresentatividadeSegundoINEP"></span>
                        </p>
                        <p class="configuracoes_optionsText">
                            Informe abaixo a porcentagem mínima para que os dados de participação da ProvaSP sejam
                            representativos. (de 0 a 100)
                        </p>
                        <input id="confRepresentatividadeSegundoINEP" style="text-align: center;" type="number" placeholder="Ex: 90" />
                    </div>

                    <!--Sync-->
                    <button id="btnSincronizarOpcoes" data-role="button">Sincronizar opções</button>
                </div>

                <p id="configuracoes_tab2" class="resultados_headerTitle" onclick="obterCorte()">
                    SEQUÊNCIA DE ATIVIDADES
                    <span id="configuracoes_icone2">
                        <span class="mdi mdi-arrow-down-drop-circle-outline"></span>
                    </span>
                </p>
                <div id="configuracoes_div2" class="configuracoes_tabDiv" style="display: none;">
                    <!--IMPORTANTE: ConfSD = Configuração de Sequência de Atividades-->
                    <table style="width:100%;">
                        <tr>
                            <td>
                                <select id="ddlConfSDEdicao" data-mini="true">
                                    <option value="" selected="selected">(Edição...)</option>
                                    <option value="2019">Edição 2019</option>
                                    <option value="2018">Edição 2018</option>
                                    <option value="2017">Edição 2017</option>
                                </select>
                            </td>
                            <td>
                                <select id="ddlConfSDAno" data-mini="true">
                                    <option value="" selected="selected">(Ano...)</option>
                                    <option value="2">2º Ano</option>
                                    <option value="3">3º Ano</option>
                                    <option value="4">4º Ano</option>
                                    <option value="5">5º Ano</option>
                                    <option value="6">6º Ano</option>
                                    <option value="7">7º Ano</option>
                                    <option value="8">8º Ano</option>
                                    <option value="9">9º Ano</option>
                                </select>
                            </td>
                        </tr>
                    </table>

                    <table style="width:100%;">
                        <tr>
                            <td>
                                <select id="ddlConfSDAreaConhecimento" data-mini="true">
                                    <option value="" selected="selected">(Área de Conhecimento...)</option>
                                    <option value="1">Ciências da Natureza</option>
                                    <option value="2">Língua Portuguesa</option>
                                    <option value="4">Redação</option>
                                    <option value="3">Matemática</option>
                                </select>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <select id="ddlConfSDCorte" data-mini="true">
                                    <option value="" selected="selected">(Corte...)</option>
                                </select>
                            </td>
                        </tr>
                    </table>
                    <button id="btnObterSequenciaDidatica" data-role="button" disabled="disabled">
                        Visualizar Sequência de Atividades
                    </button>

                    <!--Detalhes da Sequência de Atividades - CADASTRAR/ATUALIZAR -->
                    <div id="detalhesSD" style="display: none;">
                        <!--Título-->
                        <p class="configuracoes_optionsTitleLeft">Título da Sequência de Atividades</p>
                        <input id="tituloSD" type="text" placeholder="Ex: Título aqui..." />
                        <!--Text-->
                        <p class="configuracoes_optionsTitleLeft">Texto da Sequência de Atividades</p>
                        <textarea id="textSD" placeholder="Ex: Texto aqui..."></textarea>
                        <!--Link-->
                        <p class="configuracoes_optionsTitleLeft">Link</p>
                        <input id="linkSD" type="url" placeholder="Ex: Link aqui..." />

                        <!--Voltar das configurações-->
                        <button id="btnCommitSD" data-role="button">Cadastrar Sequência de Atividades</button>
                    </div>
                </div>

                <!--Voltar das configurações-->
                <button id="btnConfiguracoesSair"
                        data-role="button" style="color: rgb(205,0,0) !important;">
                    Voltar ao menu
                </button>
            </div>
        </div>
    </div>

    <!--MSTECH
    *Nova div de popup da prova do aluno. Nela será possível abrir o link com a prova física.
