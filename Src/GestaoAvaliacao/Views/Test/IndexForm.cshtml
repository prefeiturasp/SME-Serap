                    <div class="row" ng-show="showTestTAI">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="cards">

                                    @* Componente Curricular *@
                                    <div class="campoLabel">
                                        <label> {{labels.componente}}*: </label>
