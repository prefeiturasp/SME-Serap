                                                <colgroup>
                                                    <col style="width: 40%" />
                                                    <col style="width: 20%" />
                                                    <col style="width: 20%" />
                                                    <col style="width: 20%" />
                                                </colgroup>
                                                <thead>
                                                    <tr>
                                                        <th>Matriz de avaliação</th>
                                                        <th class="table-parameter-centralize">Ano</th>
                                                        <th class="table-parameter-centralize">Editar</th>
                                                        <th class="table-parameter-centralize">Excluir</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr ng-repeat="anoItem in anosItensAmostraProvaTai">
                                                        <th>{{ anoItem.Matriz.Description }}</th>
                                                        <th class="table-parameter-centralize">{{ anoItem.Ano.Description }}</th>
                                                        <th class="table-parameter-centralize">
                                                            <a ng-click="e2_editarModalAnoItensAmostraTai(anoItem)"><i type="button" class="material-icons">format_list_bulleted</i></a>
                                                        </th>
                                                        <th class="table-parameter-centralize">
                                                            <a ng-click=""><i type="button" class="material-icons" ng-click="e2_confirmarDeletarItemAnosItensAmostraProvaTai(anoItem)">delete</i></a>
                                                        </th>
