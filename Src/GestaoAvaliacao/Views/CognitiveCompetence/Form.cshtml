@* Alertas *@
<alert></alert>
<div class="wellCustom" data-ng-controller="FormCognitiveCompetenceController">
    <div class="headerCustom">
        <h4>Competência cognitiva<span>Crie uma nova competência cognitiva</span></h4>
    </div>
    <div class="well well-large itens">
        <form role="form">
            <div class="boxConteudo cards">
                <div class="form-group">
                    <div class="campoLabel">
                        <label for="cognitiveCompetence.Description">Descrição*:</label>
                        <div>
                            <input type="text" id="description" class="form-control ng-pristine ng-valid" maxlength="100" placeholder="Digite aqui a descrição" ng-model="cognitiveCompetence.Description">
                        </div>
                    </div>
                </div>
            </div>
            <div class="botoes">
                <button class="btnCancelar pull-right" ng-click="voltar()">Cancelar</button>
                <button class="btnPadrao2 pull-right" ng-click="salvar()">Salvar</button>
            </div>
        </form>
    </div>
</div>
