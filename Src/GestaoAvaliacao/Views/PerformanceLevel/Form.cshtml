@* Alertas *@
<alert></alert>
<div class="wellCustom" data-ng-controller="FormPerformanceLevelController">
    <div class="headerCustom">
        <h4>Nível de desempenho <span>Crie um novo nível de desempenho </span></h4>
    </div>
    <div class="well well-large">
        <form role="form">
            <div class="boxConteudo">
                <div class="form-group">
                    <div class="campoLabel">
                        <label for="performanceLevel.Code">Código*:</label>
                        <div>
                            <input type="text" id="value" class="form-control" maxlength="15" placeholder="Digite aqui o código" ng-model="Code">
                        </div>
                    </div>
                    <div class="campoLabel">
                        <label for="performanceLevel.Description">Descrição*:</label>
                        <div>
                            <input type="text" id="description" class="form-control" maxlength="50" placeholder="Digite aqui a descrição" ng-model="Description">
                        </div>
                    </div>
                </div>                
            </div>
        </form>
    </div>
    <div class="well well-large itens">
        <a class="btnCancelar pull-right"  href="~/PerformanceLevel/List">Cancelar</a>
        <button class="btnPadrao2 pull-right" ng-click="salvar(performanceLevel)">Salvar</button>
    </div>
</div>
